@model EA.Iws.Web.Areas.AdminExportAssessment.ViewModels.KeyDatesOverride.IndexViewModel
@using Resources = EA.Iws.Web.Areas.AdminExportAssessment.Views.KeyDatesOverride.IndexResources
@{
    ViewBag.Title = Resources.Title;
}

<h1 class="heading-large">@Resources.Heading</h1>

