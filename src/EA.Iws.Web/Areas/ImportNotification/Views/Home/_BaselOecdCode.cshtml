@model EA.Iws.Web.Areas.ImportNotification.ViewModels.Home.SummaryTableContainerViewModel
@using Resource = EA.Iws.Web.Areas.ImportNotification.Views.Home.IndexResources

<div class="overview-block">
    @if (Model.Details.WasteType != null && !Model.Details.WasteType.BaselCode.IsEmpty())
    {
        if (Model.Details.WasteType.BaselCode.IsNotApplicable)
        {
            <div class="overview-block-element">@Resource.CodesNotApplicableText</div>
        }
        else
        {
            @Html.Partial("_CodeDefinitionList", Model.Details.WasteType.BaselCode.WasteCodes)
        }
    }
    else
    {
        <div class="overview-block-element">@ViewBag.SectionIncompleteText</div>
    }
</div>