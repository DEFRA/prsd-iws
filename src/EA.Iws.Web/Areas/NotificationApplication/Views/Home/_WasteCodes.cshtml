            <table>
                <thead>
                <th>Code Type</th>
                <th>Codes</th>
                </thead>
                <tr>
                    <td>@EA.Prsd.Core.Helpers.EnumHelper.GetDisplayName(Model.BaselOecdCode[0].CodeType)</td>
                    <td>
                        @foreach (var c in @Model.BaselOecdCode)
                        {
                            @c.Code
                            <br />
                        }
                    </td>
                </tr>
