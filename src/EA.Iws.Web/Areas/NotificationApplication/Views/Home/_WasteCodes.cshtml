                    <td>@EA.Prsd.Core.Helpers.EnumHelper.GetDisplayName(Model.CustomCodes[0].CodeType)</td>
                    <td>
                        @foreach (var c in @Model.CustomCodes)
                        {
                            @c.CustomCode
