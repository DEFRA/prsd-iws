@using Resources = EA.Iws.Web.Areas.AdminImportAssessment.Views.FinancialGuaranteeDates.ReceivedAndCompletedDateResources
@model EA.Iws.Web.Areas.AdminImportAssessment.ViewModels.FinancialGuaranteeDates.ReceivedAndCompletedDateViewModel

@{
    ViewBag.Title = Resources.Title;
}

<h1 class="heading-large">@Resources.MainHeading</h1>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.Gds().ValidationSummary()
    @Html.HiddenFor(m => m.OriginalReceivedDate)

    <div class="form-group">
        @Html.Gds().LabelFor(m => m.ReceivedDate, false)
        @Html.EditorFor(m => m.ReceivedDate)
    </div>

    <div class="form-group">
        @Html.Gds().LabelFor(m => m.CompletedDate, false)
        @Html.EditorFor(m => m.CompletedDate)
    </div>

    <div class="form-group">
        <button class="button" type="submit">@Constants.ContinueButtonText</button>
    </div>
}

@section menu {
    @Html.Action("ImportNavigation", "Menu", new { area = "Admin", section = ImportNavigationSection.Assessment })
}
