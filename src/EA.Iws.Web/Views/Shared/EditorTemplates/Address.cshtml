@model EA.Iws.Web.ViewModels.Shared.Address
@{
    ViewBag.Title = "Address";
}

<div class="form-group">
    @Html.Gds().LabelFor(m => m.Building)
    @Html.Gds().TextBoxFor(m => m.Building)
</div>
<div class="form-group">
    @Html.Gds().LabelFor(m => m.Street)
    @Html.Gds().TextBoxFor(m => m.Street)
</div>
<div class="form-group">
    @Html.Gds().LabelFor(m => m.City)
    @Html.Gds().TextBoxFor(m => m.City)
</div>
<div class="form-group">
    @Html.Gds().LabelFor(m => m.County)
    @Html.Gds().TextBoxFor(m => m.County)
</div>
<div class="form-group">
    @Html.Gds().LabelFor(m => m.Postcode)
    @Html.Gds().TextBoxFor(m => m.Postcode)
</div>