<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <sectionGroup name="elmah">
      <section name="security" requirePermission="false" type="Elmah.SecuritySectionHandler, Elmah" />
      <section name="errorLog" requirePermission="false" type="Elmah.ErrorLogSectionHandler, Elmah" />
      <section name="errorMail" requirePermission="false" type="Elmah.ErrorMailSectionHandler, Elmah" />
      <section name="errorFilter" requirePermission="false" type="Elmah.ErrorFilterSectionHandler, Elmah" />
    </sectionGroup>
    <section name="emailRules" type="EA.Prsd.Email.Rules.Configuration.RulesSection, EA.Prsd.Email, Version=1.0.0.0, Culture=neutral" />
  </configSections>
  <connectionStrings>
    <add name="Iws.DefaultConnection" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=EA.Iws;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <!-- *************** -->
    <!-- Elmah settings. -->
    <!-- *************** -->
    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.disableHandleErrorFilter" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.IgnoreDefaultRoute" value="false" />
    <add key="elmah.mvc.allowedRoles" value="*" />
    <add key="elmah.mvc.allowedUsers" value="*" />
    <add key="elmah.mvc.route" value="elmah" />
    <!-- ******************** -->
    <!-- ENVIRONMENT SPECIFIC -->
    <!-- ******************** -->
    <add key="Iws.SiteRoot" value="https://localhost:44301" />
    <add key="Iws.WebSiteRoot" value="https://localhost:44302" />
    <add key="Iws.Environment" value="development" />
    <add key="Iws.VerificationEmailTestDomains" value="mailinator,hotmail,outlook" />
    <add key="Iws.MailFrom" value="noreply@iws.gov.uk" />
    <add key="Iws.SendEmail" value="true" />
    <add key="Iws.TruncateEmailAfterPlus" value="true" />
    <add key="Iws.FeedbackEmailTo" value="IWS@sfwltd.co.uk" />
    <add key="Iws.ApiSecret" value="4945361C-A026-4B28-B077-9DA715238C23" />
    <add key="Iws.ApiClientId" value="iws" />
  </appSettings>
  <!--
  Email whitelists or blacklists an be added by adding configuration in the following format
  <emailRules defaultAction="Deny">
    <add action="Allow" type="EmailAddress" value="test.user1@some.domain.com" />
    <add action="Allow" type="EmailAddress" value="another.test.user@another.domain.co.uk" />
    <add action="Allow" type="RegEx" value="^.*@environment-agency\.gov\.uk$" />
  </emailRules>
  -->
  <emailRules defaultAction="Allow" />
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules>
  </system.web>
  <runtime>
